{
    "name": "thawkins/jira-api-restclient",
    "description": "JIRA REST API Client",
    "keywords": ["jira","rest","api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shuhei Tanuma",
            "homepage": "http://thawkins.github.io/",
            "email": "thawkinseee@php.net"
        },
        {
            "name": "Tim Hawkins",
            "homepage": "http://tshawkins.blogspot.com/",
            "email": "tim.thawkins@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1|^5.0|^6.0",
        "aik099/coding-standard": "dev-master",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "thawkins\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\thawkins\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
	"config": {
		"platform": {
			"php": "5.4"
		}
	}
}
